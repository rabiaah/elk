{
  "expo": {
    "name": "Elk",
    "slug": "Elk",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_nonAlpha.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amdocs.elk",
      "config": {
        "googleMapsApiKey": "your map key here"
      }
    },
    "android": {
      "package": "com.amdocs.elk",
      "config": {
        "googleMaps": {
          "apiKey": "your map key here"
        }
      },
      "versionCode": 1
    },
    "description": ""
  }
}
